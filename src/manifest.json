{
    "manifest_version": 3,
    "name": "__MSG_Name__",
    "version": "1.6.4",
    "description": "__MSG_Description__",
    "default_locale": "en",
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "unlimitedStorage"
    ]
}