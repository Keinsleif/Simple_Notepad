{
    "manifest_version": 2,
    "name": "__MSG_Name__",
    "version": "1.5.3",
    "description": "__MSG_Description__",
    "default_locale": "en",
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["storage"]
}
